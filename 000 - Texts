--------------------------------------------------------------------------------------------------------------------
App.view.xml

<mvc:View
    controllerName="sap.ui.demo.walkthrough.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <Panel headerText="Form">
        <FlexBox
            height="auto" 
            direction="Column" 
            alignItems="Center" 
            justifyContent="Center"> 
            <Text text="Name"/>
            <Input id="in1" placeholder="Enter your text here..." width="100%"/>
            <Text text="Age"/>
            <Input id="in2" placeholder="Enter your text here..." width="100%"/>
            <Text text="Job"/>
            <Input id="in3" placeholder="Enter your text here..." width="100%"/>
            <Button text="Save" press=".showMsg"/>
        </FlexBox>
    </Panel>
</mvc:View>

--------------------------------------------------------------------------------------------------------------------
View1.view.xml

<mvc:View
    controllerName="webapp.controller.View1"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
    <Page id="page" title="TitleMenu">
        <content>
            <Button id="_IDGenButton1aa" text="Register" press="myScreen"></Button>
        </content>
    </Page>
</mvc:View>
